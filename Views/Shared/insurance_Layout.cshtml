                @if(User.Identity.IsAuthenticated){

                    var username = User.Claims.Where(x => x.Type == System.Security.Claims.ClaimTypes.Name)
                    .Select(x => x.Value)
                    .FirstOrDefault();

                    

                    var Role = User.Claims.Where(x => x.Type == System.Security.Claims.ClaimTypes.Role)
                    .Select(x => x.Value)
                    .FirstOrDefault();

                        @if (Role == "Admin")
                        {
                        <a asp-controller="admin" asp-action="owner" class="nav-item nav-link">GoTo Dashboard</a>
                    }

                    else
                    {
                        <li class="nav-item dropdown">
                            <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">@username</a>
                            <ul class="dropdown-menu bg-light border-0 m-0">
                                <a asp-controller="Account" asp-action="Signout" class="nav-item nav-link">LogOut</a>
                            </ul>

                        </li>
                    }




                     

                }

